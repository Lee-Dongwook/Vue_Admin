    "plus-pro-components": "^0.1.21",
    "qrcode": "^1.5.4",
    "qs": "^6.14.0",
    "responsive-storage": "^2.2.0",
    "sortablejs": "^1.15.6",
    "typeit": "^8.8.7",
    "version-rocket": "^1.7.4",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.2",
    "vue-router": "^4.5.0",
    "vue-tippy": "^6.6.0",
    "vue-types": "^6.0.0",
    "vxe-table": "^4.11.19",
    "xlsx": "^0.18.5"
